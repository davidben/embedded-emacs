{
  "name": "Embedded Emacs",
  "version": "1.0",
  "description": "Replaces textareas with an embedded Emacs.",
  "permissions": [
  ],
  "background_page": "background.html",
  "content_scripts": [ {
    "matches": ["<all_urls>"],
    "js": ["content_script.js"],
    "run_at": "document_idle",
    "all_frames": true
  } ],
  "plugins": [
    { "path": "libemacs-npapi.so", "public": true }
  ]
}
